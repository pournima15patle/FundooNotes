{
  "name": "notes",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string",
      "required": false
    },
    "discription": {
      "type": "string",
      "required": true
    },
    "color": {
      "type": "string",
      "required": false
    },
    "isTrash": {
      "type": "boolean",
      "required": false,
      "default": "false"
    },
    "isArchive": {
      "type": "boolean",
      "required": false,
      "default": "false"
    },
    "logTime": {
      "type": "date",
      "dataType": "timestamp",
      "defaultFn": "now"
    },
    "reminder": {
      "type": "date",
      "required": false
    },
    "label": {
      "type": "object",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "userModel": {
      "type": "belongsTo",
      "model": "userModel",
      "foreignKey": ""
    },
    "labels": {
      "type": "hasMany",
      "model": "label",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
